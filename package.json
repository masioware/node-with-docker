{
  "name": "nodejs-docker-template",
  "version": "1.0.0",
  "description": "Simple template for NodeJS development with Docker",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon -L --inspect=0.0.0.0:9229 --nolazy src/server.js",
    "build": "docker build -t nodejs-docker-template:latest .",
    "up": "docker compose up",
    "up:detached": "docker compose up -d",
    "down": "docker compose down"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "mysql2": "^2.3.3",
    "nodemon": "^2.0.20",
    "sequelize": "^6.23.2"
  }
}
